[alias]
	diff = diff -w
	co = checkout
	ci = commit
	st = status
	br = branch
	lg = log --oneline
	di = diff --name-status
	amend = commit --amend -C HEAD
	lg1 = log --name-status HEAD^..HEAD
	lg2 = log --name-status HEAD~2..HEAD
	di1 = diff -w HEAD^..HEAD
